{
  "OnlyAdmin": {
    "description": "仅管理员可用",
    "hint": "仅机器人管理员可通过命令获取用户信息。",
    "type": "bool",
    "default": false
  },
  "BoxBlacklist": {
    "description": "信息保护用户",
    "type": "list",
    "hint": "处于该列表中的用户不会被通过命令获取到用户信息，但这不影响自动获取。",
    "default": []
  },
  "WhitelistGroups": {
    "description": "群聊白名单",
    "type": "list",
    "default": [],
    "hint": "仅处理白名单群聊中的命令，留空则处理所有群聊的请求。"
  },
  "AutoBoxConfig": {
    "description": "自动获取配置",
    "type": "object",
    "items": {
      "AutoBoxConfig_IncreaseBox": {
        "description": "自动获取新进群的用户信息",
        "type": "bool",
        "default": false
      },
      "AutoBoxConfig_DecreaseBox": {
        "description": "自动获取退群用户的信息",
        "type": "bool",
        "default": false
      },
      "AutoBoxConfig_WhiteGroups": {
        "description": "群聊白名单",
        "type": "list",
        "hint": "只自动获取白名单群聊的新群友/主动退群的用户，不填则所有群聊都启用自动获取用户信息。",
        "default": []
      }
    }
  },
  "RateLimitConfig": {
    "type": "object",
    "description": "速率限制配置",
    "items": {
      "RateLimitConfig_Time": {
        "description": "速率限制时间",
        "type": "int",
        "hint": "使用命令的最小间隔时间，单位为分钟，设为0表示不限制。",
        "default": 0
      },
      "RateLimitConfig_WhiteGroups": {
        "description": "群聊白名单",
        "type": "list",
        "hint": "白名单群聊不受速率限制。",
        "default": []
      },
      "RateLimitConfig_WhiteUsers": {
        "description": "用户白名单",
        "type": "list",
        "hint": "白名单用户在任何群聊中都不受速率限制。",
        "default": []
      }
    }
  },
  "DisplayConfig":{
    "description": "功能显示设置",
    "type": "object",
    "hint": "只有能够获取到的信息才会展示，未获取到的信息即使启用了也不会展示的。",
    "obvious_hint": true,
    "items": {
      "DisplayConfig_Card": { 
        "description": "显示用户群昵称",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_Title": {
        "description": "显示用户群头衔",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_Sex": {
        "description": "显示用户性别",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_BirthdayConfig": {
        "description": "生日显示设置",
        "type": "object",
        "items": {
          "BirthdayConfig_Enable": {
            "description": "显示用户生日",
            "type": "bool",
            "default": true
          },
          "BirthdayConfig_Constellation": {
            "description": "显示用户星座",
            "type": "bool",
            "hint": "此数据通过用户生日计算得出，若不开启“显示用户生日”则此项无效。",
            "default": true
          },
          "BirthdayConfig_Zodiac": {
            "description": "显示用户生肖",
            "type": "bool",
            "hint": "此数据通过用户生日计算得出，若不开启“显示用户生日”则此项无效。",
            "default": true
          }
        }
      },
      "DisplayConfig_Age": {
        "description": "显示用户年龄",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_PhoneNum": {
        "description": "显示用户手机号码",
        "type": "bool",
        "default": true,
        "hint": "如果你不打算将机器人添加至黑名单，我们建议你关闭此项，否则这会导致机器人使用的注册电话号码泄露。",
        "obvious_hint": true
      },
      "DisplayConfig_Email": {
        "description": "显示用户电子邮箱",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_PostCode": {
        "description": "显示用户邮政编码",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_HomeTown": {
        "description": "显示用户家乡城市",
        "type": "bool",
        "default": true,
        "hint": "由于API限制，信息显示可能不够精确。"
      },
      "DisplayConfig_Address": {
        "description": "显示用户现居城市",
        "type": "bool",
        "default": true,
        "hint": "由于API限制，信息显示可能不够精确。"
      },
      "DisplayConfig_BloodType": {
        "description": "显示用户血型",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_Career": {
        "description": "显示用户职业",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_Remark": {
        "description": "显示用户备注",
        "type": "bool",
        "default": true,
        "hint": "显示机器人账号上对该用户的备注。"
      },
      "DisplayConfig_Labels": {
        "description": "显示用户个性标签",
        "type": "bool",
        "default": true,
        "hint": "该选项可能无法正常工作。"
      },
      "DisplayConfig_Unfriendly": {
        "description": "显示用户是否为风险账号",
        "type": "bool",
        "default": true,
        "hint": "该选项可能无法正常工作。"
      },
      "DisplayConfig_VipConfig": {
        "description": "VIP显示设置相关",
        "type": "object",
        "items": {
          "VipConfig_Enable": {
            "description": "是否显示用户VIP信息",
            "type": "bool",
            "default": true
          },
          "VipConfig_YearsVip": {
            "description": "是否显示用户年VIP信息",
            "type": "bool",
            "default": true,
            "hint": "若“是否显示用户VIP信息”关闭则此项无效。"
          },
          "VipConfig_VipLevel": {
            "description": "是否显示用户VIP等级",
            "type": "bool",
            "default": true,
            "hint": "若“是否显示用户VIP信息”关闭则此项无效。"
          }
        }
      },
      "DisplayConfig_LoginDays": {
        "description": "显示用户连续登录天数",
        "type": "bool",
        "default": true
      },
      "DisplayConfig_Level": {
        "description": "显示用户群等级",
        "type": "bool",
        "default": true,
        "hint": "该用户若不在群内则无效。"
      },
      "DisplayConfig_JoinTime": {
        "description": "显示用户加群时间",
        "type": "bool",
        "default": true,
        "hint": "该用户若不在群内则无效。"
      },
      "DisplayConfig_LongNick": {
        "description": "显示用户个性签名",
        "type": "bool",
        "default": true
      }
    }
  }
}